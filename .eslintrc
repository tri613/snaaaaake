{
  "plugins": [
    "prettier"
  ],
  "extends": [
    "prettier",
    "plugin:vue/essential"
  ],
  "rules": {
    "prettier/prettier": "off",
    "vue/html-self-closing": 0
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}